{
  "plugins": [],
  "recurseDepth": 10,
  "source": {
    "includePattern": ["./lib/.+\\.js(doc|x)?$", "../*/.+\\.js(doc|x)?$"],
    "excludePattern": "(^|\\/|\\\\)_"
  },
  "sourceType": "module",
  "tags": {
    "allowUnknownTags": true,
    "dictionaries": ["jsdoc","closure"]
  },
  "templates": {
    "cleverLinks": false,
    "monospaceLinks": false
  }
}