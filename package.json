{
  "name": "@dboxjs/core",
  "version": "0.0.9",
  "description": "A library to create easy reusable charts",
  "main": "dist/dbox.js",
  "module": "dist/dbox.mjs",
  "jsnext:main": "dist/dbox.mjs",
  "scripts": {
    "eslint": "eslint -c .eslintrc lib test",
    "build": "rollup -c && uglifyjs ./dist/dbox.js -c -m -o ./dist/dbox.min.js",
    "dev": "rollup -c rollup.config.dev.js -w",
    "pretest": "npm run build",
    "test": "mocha",
    "docs": "jsdoc -c jsdocs.config.json",
    "prepublish": "npm test"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/dboxjs/core.git"
  },
  "keywords": [
    "d3",
    "charts",
    "dataviz",
    "dbox",
    "dboxjs"
  ],
  "author": "",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/dboxjs/dbox/issues"
  },
  "files": [
    "lib",
    "dist",
    "docs"
  ],
  "homepage": "https://github.com/dboxjs/dbox#readme",
  "prettier": {
    "singleQuote": true
  },
  "dependencies": {
    "d3": "^4.13.0",
    "d3-queue": "^3.0.7",
    "d3-tip": "^0.9.1",
    "lodash": "^4.17.15",
    "textures": "^1.0.4",
    "topojson": "^2.2.0"
  },
  "devDependencies": {
    "@dboxjs/bars": "^0.x",
    "@dboxjs/distro": "^0.x",
    "@dboxjs/heatmap": "^0.x",
    "@dboxjs/leaflet": "^0.x",
    "@dboxjs/map": "^0.x",
    "@dboxjs/radar": "^0.x",
    "@dboxjs/scatter": "^0.x",
    "@dboxjs/spineplot": "^0.x",
    "@dboxjs/timeline": "^0.x",
    "@dboxjs/treemap": "^0.x",
    "babel": "^6.23.0",
    "babel-eslint": "^7.1.1",
    "babel-plugin-external-helpers": "^6.22.0",
    "babel-preset-env": "^1.6.0",
    "babel-register": "^6.18.0",
    "babelrc-rollup": "^3.0.0",
    "eslint": "^4.18.2",
    "istanbul": "^0.4.5",
    "jsdoc": "^3.6.3",
    "mocha": "^5.1.1",
    "mocha-phantomjs": "^4.1.0",
    "redux": "^4.0.1",
    "rollup": "^0.57.1",
    "rollup-plugin-babel": "^3.0.7",
    "rollup-plugin-commonjs": "^9.1.0",
    "rollup-plugin-istanbul": "^1.1.0",
    "rollup-plugin-legacy": "^1.0.0",
    "rollup-plugin-node-resolve": "^2.1.1",
    "rollup-watch": "^2.5.0",
    "uglify-js": "^2.8.10"
  },
  "directories": {
    "doc": "docs",
    "test": "test"
  }
}
